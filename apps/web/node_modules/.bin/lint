else
  export NODE_PATH="/Users/francismasha/Documents/Projects/Personal/plutus/node_modules/.pnpm/@million+lint@0.0.73_@types+react-dom@18.3.0_@types+react@18.3.2_immer@10.1.1_rollup@4.17.2/node_modules/@million/lint/dist/wizard/node_modules:/Users/francismasha/Documents/Projects/Personal/plutus/node_modules/.pnpm/@million+lint@0.0.73_@types+react-dom@18.3.0_@types+react@18.3.2_immer@10.1.1_rollup@4.17.2/node_modules/@million/lint/dist/node_modules:/Users/francismasha/Documents/Projects/Personal/plutus/node_modules/.pnpm/@million+lint@0.0.73_@types+react-dom@18.3.0_@types+react@18.3.2_immer@10.1.1_rollup@4.17.2/node_modules/@million/lint/node_modules:/Users/francismasha/Documents/Projects/Personal/plutus/node_modules/.pnpm/@million+lint@0.0.73_@types+react-dom@18.3.0_@types+react@18.3.2_immer@10.1.1_rollup@4.17.2/node_modules/@million/node_modules:/Users/francismasha/Documents/Projects/Personal/plutus/node_modules/.pnpm/@million+lint@0.0.73_@types+react-dom@18.3.0_@types+react@18.3.2_immer@10.1.1_rollup@4.17.2/node_modules:/Users/francismasha/Documents/Projects/Personal/plutus/node_modules/.pnpm/node_modules:$NODE_PATH"
