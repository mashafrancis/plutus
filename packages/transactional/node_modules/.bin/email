#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/francismasha/Documents/Projects/Personal/plutus/node_modules/.pnpm/react-email@2.1.2_@opentelemetry+api@1.8.0_@swc+helpers@0.5.11_eslint@9.2.0/node_modules/react-email/cli/node_modules:/Users/francismasha/Documents/Projects/Personal/plutus/node_modules/.pnpm/react-email@2.1.2_@opentelemetry+api@1.8.0_@swc+helpers@0.5.11_eslint@9.2.0/node_modules/react-email/node_modules:/Users/francismasha/Documents/Projects/Personal/plutus/node_modules/.pnpm/react-email@2.1.2_@opentelemetry+api@1.8.0_@swc+helpers@0.5.11_eslint@9.2.0/node_modules:/Users/francismasha/Documents/Projects/Personal/plutus/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/francismasha/Documents/Projects/Personal/plutus/node_modules/.pnpm/react-email@2.1.2_@opentelemetry+api@1.8.0_@swc+helpers@0.5.11_eslint@9.2.0/node_modules/react-email/cli/node_modules:/Users/francismasha/Documents/Projects/Personal/plutus/node_modules/.pnpm/react-email@2.1.2_@opentelemetry+api@1.8.0_@swc+helpers@0.5.11_eslint@9.2.0/node_modules/react-email/node_modules:/Users/francismasha/Documents/Projects/Personal/plutus/node_modules/.pnpm/react-email@2.1.2_@opentelemetry+api@1.8.0_@swc+helpers@0.5.11_eslint@9.2.0/node_modules:/Users/francismasha/Documents/Projects/Personal/plutus/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-email/cli/index.js" "$@"
else
  exec node  "$basedir/../react-email/cli/index.js" "$@"
fi
