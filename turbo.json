{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env"
  ],
  "pipeline": {
    "topo": {
      "dependsOn": [
        "^topo"
      ]
    },
    "build": {
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.development.local",
        ".env.local",
        ".env.development",
        ".env"
      ]
    },
    "start": {
      "cache": false
    },
    "check-types": {},
    "format": {
      "outputMode": "new-only"
    },
    "lint": {
      "dependsOn": [
        "^topo"
      ]
    },
    "clean": {
      "cache": false
    },
    "db:setup": {
      "cache": false
    }
  }
}
